import random

number = random.randint(1, 10)
guess = 0

print("Guess the number between 1 and 10")

while guess != number:
    guess = int(input("Enter your guess: "))

    if guess < number:
        print("Too Low!")
    elif guess > number:
        print("Too High!")
    else:
        print("ğŸ‰ Correct! You guessed the number.")